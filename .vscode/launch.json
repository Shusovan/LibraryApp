    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Python Debugger: AdminService",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "AdminService.admin_service:app",
                "--port",
                "8001",
                "--reload"
            ],
            "jinja": true
        },

        {
            "name": "Python Debugger: SecurityService",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "SecurityService.security_service:app",
                "--reload"
            ],
            "jinja": true
        }
    ]
}